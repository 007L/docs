<?js
  var data = obj;
  var self = this;
  var coreHeader = false;
  var pluginHeader = false;
  var utilHeader = false;
?>
<div class="navigation">
  <h3 class="applicationName">
    <a href="index.html"><?js= env.conf.templates.applicationName.replace(/\s+/g, '') ?></a>
    <span class="version"><?js= env.opts.query.version ?></span>
  </h3>

  <div class="search">
    <input id="search" type="text" class="form-control input-sm" placeholder="Search Documentations">
  </div>
  <ul class="list">

  <!-- Tutorials -->
  <?js this.members.tutorials.forEach(function (tutorial) { ?>
    <?js tutorial.children.forEach(function (child) { ?>
      <li class="item" data-name="tutorial-<?js= child.name ?>">
        <span class="title">
          <a href="#"><?js= child.title ?></a>
        </span>
        <ul class="members itemMembers" style="margin-top: 5px;">
          <?js child.children.forEach(function (c) { ?>
            <li class="sub-item" data-name="tutorial-<?js= c.name ?>"><?js= self.tutoriallink(c.name) ?></li>
          <?js }); ?>
        </ul>
      </li>
    <?js }); ?>
  <?js }) ?>

  <li class="api-reference" style="color: #4267B6; font-weight: bold; margin-top: 12px; margin-bottom: 9px;">
    API Reference
  </li>

  <!-- Core -->
  <?js this.nav.core.forEach(function (item) { ?>
    <?js if (!coreHeader) { ?>
      <?js coreHeader = true; ?>
      <li class="header" style="color: #779c34; margin-top: 15px; margin-bottom: 5px; font: bold 0.7em Verdana, Helvetica; display: block;">
          Core
      </li>
    <?js } ?>
    <li class="item" data-name="<?js= item.longname ?>">
      <span class="title">
        <a href="#"><?js= item.longname ?></a>
        <?js if (item.type === 'namespace') { ?>
          <span class="static">static</span>
        <?js } ?>
      </span>
      <ul class="members itemMembers">
        <?js if (item.members.length) { ?>
          <span class="subtitle">Members</span>
          <?js item.members.forEach(function (v) { ?>
            <li data-name="<?js= v.longname ?>"><?js= self.linkto(v.longname, v.name) ?></li>
          <?js }); ?>
        <?js } ?>
      </ul>
      <ul class="typedefs itemMembers">
